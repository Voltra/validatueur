		"url": "https://github.com/Voltra/validatueur/issues"
	},
	"homepage": "https://github.com/Voltra/validatueur#readme",
	"devDependencies": {
		"@types/jest": "^25.2.3",
		"compression-webpack-plugin": "^6.0.3",
		"friendly-errors-webpack-plugin": "^1.7.0",
		"jest": "^26.5.3",
		"null-loader": "^4.0.1",
		"prettier": "^2.1.2",
		"ts-jest": "^26.4.1",
		"ts-loader": "^7.0.4",
		"typedoc": "^0.17.7",
		"typedoc-plugin-as-member-of": "^1.0.2",
		"typescript": "^3.9.2",
		"webpack": "^4.44.2",
		"webpack-bundle-analyzer": "^3.9.0",
		"webpack-cli": "^3.3.11",
		"webpack-progress-bar": "^1.2.1"
	},
	"dependencies": {
		"deepcopy": "^2.1.0",
		"moment": "^2.29.1",
