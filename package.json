	"description": "Feature complete validation and sanitization library",
	"main": "dist/index.js",
	"types": "lib/index.d.ts",
	"scripts": {
		"build": "webpack --colors",
		"watch": "webpack --colors --watch",
		"start": "npm run build",
		"test": "jest",
		"tdd": "jest --watchAll",
		"format": "prettier --config prettier.config.js --write 'src/**/*.ts' 'tests/**/**.ts'",
		"docs": "typedoc --options typedoc.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Voltra/validatueur.git"
	},
	"keywords": [
